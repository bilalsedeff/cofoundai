# CoFound.ai Environment Configuration
# Copy this file to .env and fill in your actual values

# LLM Configuration
LLM_PROVIDER=test
MODEL_NAME=gpt-4o
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Development Mode (test mode without API keys)
DEVELOPMENT_MODE=true

# Google Cloud Platform
GOOGLE_APPLICATION_CREDENTIALS=
GOOGLE_CLOUD_PROJECT=cofound-ai-project

# Google Cloud Vertex AI (GPU Accelerated)
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_REGION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account.json
VERTEX_AI_MODEL=gemini-pro
ENABLE_GPU_ACCELERATION=true

# Google Cloud Platform
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_REGION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account.json

# Database Configuration
DATABASE_USER=cofoundai_user
DATABASE_PASSWORD=your_secure_password
DATABASE_NAME=cofoundai
DATABASE_HOST=localhost
DATABASE_PORT=5432

# Development Configuration
DEVELOPMENT_MODE=true
DEBUG=true
LOG_LEVEL=INFO

# LangSmith (Optional)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=cofoundai

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
CORS_ORIGINS=*

# Frontend Configuration
FRONTEND_URL=http://localhost:3000

# Secret Keys
SECRET_KEY=your_secret_key_for_sessions
JWT_SECRET=your_jwt_secret_key

# Monitoring & Analytics
ENABLE_MONITORING=true
ENABLE_ANALYTICS=true

# Cache Configuration
REDIS_URL=redis://localhost:6379/0
CACHE_TTL=3600

# Storage Configuration
STORAGE_BUCKET=cofoundai-storage
ARTIFACT_STORAGE_PATH=./artifacts

# Worker Configuration
WORKER_CONCURRENCY=4
TASK_TIMEOUT=300

# Feature Flags
ENABLE_EXTENSIBLE_ARCHITECTURE=true
ENABLE_MICROSERVICES=false
ENABLE_CLOUD_DEPLOYMENT=false